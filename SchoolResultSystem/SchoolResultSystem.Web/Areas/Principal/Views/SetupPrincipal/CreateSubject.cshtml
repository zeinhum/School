@{
    ViewData["Title"] = "Admin Dashboard";
    Layout = null;
}
<link rel="stylesheet" href="~/css/form.css" />

<script type="module" src="/js/addClasses.js"></script>

<div class="dashboard-container">
    <form class="form-container" action="/Principal/SetupPrincipal/SaveSubject" method="post">
        <p class="error"></p>
        <h2>Add Subjects those are taught in the school</h2>

        <table class="form-card">
            
            <thead>
                <tr>
                    <th>Subject Code</th>
                    <th>Subject Name</th>
                    <th>add</th>
                </tr>
            </thead>

            <tbody class="form-data">
                <tr>
                    
                    <td><input type="text" name="SCode" required size="2" placeholder="Subject Code"/></td>
                    <td><input type="text" name="SName" required size="2" placeholder="Subject Name"/></td>

                </tr>
            </tbody>
        </table>

        <button type="submit" class="btn-form btn-success">Save Subject</button>
    </form>

    <partial name="_RightNavBar" model="-1" />
</div>